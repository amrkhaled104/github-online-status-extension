{
    "manifest_version": 3,
    "name": "GitHub Friends Online",
    "version": "1.0",
    "permissions": [
        "storage"
    ],
    "host_permissions": [
        "https://github.com/*"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://github.com/*"
            ],
            "js": [
                "content.js"
            ],
            "css": [
                "style.css"
            ]
        }
    ]
}